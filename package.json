{
  "name": "apollo11",
  "version": "1.0.0",
  "description": "apolo client dev tool",
  "main": "index.js",
  "scripts": {
    "build:contentscript-background": "NODE_ENV=production webpack --config webpack.config.js",
    "build": "npm run build:contentscript-background",
    "watch:contentscript-background": "NODE_ENV=development webpack --config webpack.config.js -w",
    "watch": "npm-run-all --parallel watch:*",
    "clean": "rm -rf dist && cpy manifest.json dist && cpy src/assets/* dist/assets",
    "prebuild": "npm run clean",
    "prewatch": "npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/apollo11"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oslabs-beta/apollo11/issues"
  },
  "homepage": "https://github.com/oslabs-beta/apollo11#readme",
  "devDependencies": {
    "@types/chrome": "0.0.122",
    "@types/node": "^14.0.27",
    "awesome-typescript-loader": "^5.2.1",
    "cpy-cli": "^3.1.1",
    "css-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
